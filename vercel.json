{
  "version": 2,
  "builds": [
    {
      "src": "api/auth.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/forms.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/forms/(.*)",
      "dest": "/api/forms.js"
    },
    {
      "src": "/api/health/(.*)",
      "dest": "/api/health.js"
    },
    {
      "src": "/",
      "dest": "/api/health.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/auth.js": {
      "maxDuration": 30
    },
    "api/forms.js": {
      "maxDuration": 30
    },
    "api/health.js": {
      "maxDuration": 10
    }
  }
}